{
  "name": "@console/bluemix-components-react",
  "version": "0.0.8-beta",
  "description": "A React wrapper for bluemix-components",
  "main": "index.js",
  "scripts": {
    "check": "npm run lint && npm run test",
    "coverage": "jest --coverage && node tools/create-badge.js",
    "lint": "eslint .",
    "storybook": "start-storybook -p 9001",
    "test": "jest",
    "test-watch": "jest --watch",
    "deploy": "build-storybook -s .storybook -o .gh-pages && node deploy.js"
  },
  "keywords": [
    "react",
    "bluemix",
    "bluemix-components"
  ],
  "files": [
    "components/**/*",
    "containers/**/*",
    "elements/**/*",
    "internal/**/*",
    "lib/**/*"
  ],
  "contributors": [
    {
      "name": "Brian Han",
      "email": "bthan@us.ibm.com"
    },
    {
      "name": "Aziz Punjani",
      "email": "punjani@us.ibm.com"
    },
    {
      "name": "Mari Johannessen",
      "email": "mjohannessen@us.ibm.com"
    },
    {
      "name": "T.J. Eagan",
      "email": "twegan@us.ibm.com"
    },
    {
      "name": "Ian Fleming",
      "email": "ianfleming@us.ibm.com"
    },
    {
      "name": "Chris Dhanaraj",
      "email": "chrisdhanaraj@us.ibm.com"
    }
  ],
  "license": "MIT",
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "max-len": [
        2,
        130,
        4
      ],
      "no-underscore-dangle": 0,
      "react/prefer-stateless-function": 0
    },
    "env": {
      "jest": true
    }
  },
  "dependencies": {
    "@console/bluemix-components": "6.7.0",
    "@console/bluemix-icons": "2.1.0",
    "classnames": "2.2.5",
    "json-loader": "0.5.4",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "3.2.1",
    "@kadira/react-storybook-decorator-centered": "^1.0.0",
    "@kadira/storybook": "1.38.0",
    "autoprefixer": "^6.3.7",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^14.1.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "gh-pages": "0.11.0",
    "jest": "^15.1.1",
    "lcov2badge": "^0.1.0",
    "node-sass": "3.8.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.3.1",
    "sass-loader": "4.0.0",
    "style-loader": "0.13.1",
    "warning": "3.0.0"
  },
  "jest": {
    "coverageDirectory": "./.gh-pages/coverage",
    "moduleNameMapper": {
      "^.+\\.(scss)$": "<rootDir>/lib/styleMock.js"
    }
  }
}
